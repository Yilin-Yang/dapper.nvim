Execute (VariablesPrinter: Parse Structured Variable):
  let g:parsed = dapper#view#VariablesPrinter#VariableFromString(
      \ '  v quoz, Foo, {class}:')
  AssertEqual {
      \ 'indentation': '  ',
      \ 'expanded': 1,
      \ 'unstructured': 0,
      \ 'name': 'quoz',
      \ 'type': 'Foo',
      \ 'presentation_hint': '{class}',
      \ 'value': '',
      \ }, g:parsed

Execute (VariablesPrinter: Parse Unstructured Variable):
  let g:parsed = dapper#view#VariablesPrinter#VariableFromString(
      \ '  - foo, int: 123')
  AssertEqual {
      \ 'indentation': '  ',
      \ 'expanded': 0,
      \ 'unstructured': 1,
      \ 'name': 'foo',
      \ 'type': 'int',
      \ 'presentation_hint': '',
      \ 'value': '123',
      \ }, g:parsed

