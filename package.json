  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "es6-promise": "^4.2.5",
    "mocha": "^5.2.0",
    "neovim": "^4.2.1",
    "ts-node": "^7.0.1"
  }
